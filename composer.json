{
    "name": "cipemotion/medialibrary",
    "description": "A Laravel media library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bouma",
            "email": "me@alexbouma.me"
        },
        {
            "name": "Joris van Willigen",
            "email": "me@jorisvanw.me"
        }
    ],
    "suggest": {
        "aws/aws-sdk-php": "To use the S3PresignedUrlGenerator."
    },
    "require": {
        "php": ">=7.1",
        "ramsey/uuid": "^3.5|^4.0",
        "statamic/stringy": "^2.8|^3.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "laravel/framework": "^5.3|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "intervention/image": "^2.3",
        "cloudconvert/cloudconvert-php": "^2.2|^3.2"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.19"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "CipeMotion\\Medialibrary\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CipeMotion\\Medialibrary\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
