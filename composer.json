{
    "name": "cipemotion/medialibrary",
    "description": "A Laravel media library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bouma",
            "email": "me@alexbouma.me"
        },
        {
            "name": "Joris van Willigen",
            "email": "me@jorisvanw.me"
        }
    ],
    "suggest": {
        "aws/aws-sdk-php": "To use the S3PresignedUrlGenerator."
    },
    "require": {
        "ramsey/uuid": "^3.5",
        "intervention/image": "^2.3",
        "illuminate/support": "^5.3|^5.4",
        "illuminate/database": "^5.3|^5.4",
        "danielstjules/stringy": "^2.3",
        "cloudconvert/cloudconvert-php": "^2.2"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.19"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "CipeMotion\\Medialibrary\\": "src"
        }
    },
    "minimum-stability": "stable"
